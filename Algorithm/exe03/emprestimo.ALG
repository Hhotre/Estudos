algoritmo "semnome"
var
   emprestimo, parcela: real
inicio
      Escreval("Quanto de emprestimo voce quer pegar? ")
      leia(emprestimo)
      Escreval("Quantas parcelas? ")
      leia(parcela)
      emprestimo <- parcela 100)
      Escreval("Ira pagar ",parcela," parcela de R$",emprestimo)
fimalgoritmo